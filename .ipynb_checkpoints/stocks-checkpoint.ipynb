   "source": [
    "from bokeh.plotting import figure, output_file, show\n",
    "import pandas\n",
    "\n",
    "df = pandas.read_csv(\"stocks.csv\", parse_dates=[\"Date\"])\n",
    "\n",
    "x = df[\"Date\"]\n",
    "y = df[\"Close\"]\n",
    "\n",
    "p = figure()\n",
    "\n",
    "p.line(x, y)\n",
    "\n",
    "output_file(\"stocks_plot.html\")\n",
    "\n",
    "show(p)"
   ]
